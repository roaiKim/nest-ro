  "scripts": {
    "prebuild": "rimraf public",
    "build": "nest build & yarn build:view",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node public/service/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./service/test/jest-e2e.json",
    "view": "yarn start:view",
    "build:view": "webpack --config client/webpack/webpack.config.build.js",
    "start:view": "webpack-dev-server --config client/webpack/webpack.config.dev.js"
  },
